{
  "expo": {
    "name": "Alertraa",
    "slug": "alertraa-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alertraa.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alertraa.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "display": "standalone",
      "scope": "/Alertraa_startup_UIUX",
      "startUrl": "/Alertraa_startup_UIUX",
      "orientation": "portrait-primary",
      "theme_color": "#1a73e8",
      "background_color": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "700bae58-d38a-440a-a0e0-891dec8c385e"
      }
    }
  }
}
